{
  "projectType": "library",
  "sourceRoot": "libs/xl01/shared/styles/src",
  "prefix": "x01-v1",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/xl01/shared/styles"],
      "options": {
        "jestConfig": "libs/xl01/shared/styles/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/xl01/shared/styles/**/*.ts",
          "libs/xl01/shared/styles/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:styles-lib", "type:xl01-lib"]
}
