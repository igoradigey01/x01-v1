{
  "projectType": "library",
  "sourceRoot": "libs/shared-styles/src",
  "prefix": "x01-v1",
  "targets": {
    "build": {
      "options": {
        "styles": [
          "libs/shared-styles/src/style.scss",
          "libs/shared-styles/src/util.scss"
        ]
      }},
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared-styles"],
      "options": {
        "jestConfig": "libs/shared-styles/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared-styles/**/*.ts",
          "libs/shared-styles/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:shared-styles-lib", "type:style-lib"]
}
